<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="sonata.admin.post" class="AppBundle\Admin\DreamAdmin">
            <tag name="sonata.admin" manager_type="doctrine_mongodb" group="Content" label="Dream"/>
            <argument />
            <argument>AppBundle\Document\Dream</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>AppBundle</argument>
            </call>
        </service>

        <service id="sonata.admin.faq" class="AppBundle\Admin\FaqAdmin">
            <tag name="sonata.admin" manager_type="doctrine_mongodb" group="Content" label="Faq"/>
            <argument />
            <argument>AppBundle\Document\Faq</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>AppBundle</argument>
            </call>
        </service>

        <service id="sonata.admin.status" class="AppBundle\Admin\StatusAdmin">
            <tag name="sonata.admin" manager_type="doctrine_mongodb" group="Content" label="Status"/>
            <argument />
            <argument>AppBundle\Document\Status</argument>
            <argument />
            <call method="setTranslationDomain">
                <argument>AppBundle</argument>
            </call>
        </service>

    </services>
</container>
